{
  "name": "git",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "react": "^16.8.0-alpha.1"
  },
  "scripts": {
    "commit": "npx git-cz",
    "commitmsg": "validate-commit-msg"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "gulp lint",
      "commit-msg": "validate-commit-msg"
  }
  },
  "devDependencies": {
    "ghooks": "^2.0.4",
    "validate-commit-msg": "^2.14.0",
    "yorkie": "^2.0.0"
  }
}
