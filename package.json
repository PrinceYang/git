{
  "name": "git",
  "version": "0.1.5",
  "private": true,
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "conventional-changelog": "^3.0.6",
    "react": "^16.8.0-alpha.1"
  },
  "scripts": {
    "commit": "npx git-cz",
    "changelog": "conventional-changelog -p angular -I CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "validate-commit-msg": "^2.14.0"
  }
}
